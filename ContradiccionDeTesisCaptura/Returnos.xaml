<Window x:Class="ContradiccionDeTesisCaptura.Returnos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        Title="Returnos"
        Width="583"
        Height="428" Loaded="Window_Loaded">
    <Grid>
        <Label Name="label1"
               Height="28"
               Margin="31,25,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Fecha del returno:" />
        <telerik:RadDatePicker Name="RDateReturno"
                               Width="146"
                               Margin="144,22,0,0"
                               SelectedValue="{Binding Fecha}"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top" />
        <GroupBox Name="groupBox1"
                  Width="503"
                  Height="134"
                  Margin="31,59,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  Header="Datos del origen">
            <Grid>
                <Label Name="label2"
                       Height="28"
                       Margin="19,6,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       Content="Organismo" />
                <ComboBox Name="CbxOrigen"
                          Width="446"
                          Height="23"
                          Margin="19,29,0,0"
                          ItemsSource="{Binding}"
                          DisplayMemberPath="Organismo"
                          SelectedValuePath="IdOrganismo"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top" />
                <Label Name="label3"
                       Height="28"
                       Margin="19,69,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       Content="Expediente:" />
                <TextBox Name="TxtExpOrigen"
                         Width="239"
                         Height="23"
                         Margin="86,66,0,0"
                         Text="{Binding ExpOrigen}"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <GroupBox Name="groupBox2"
                  Width="503"
                  Height="134"
                  Margin="31,210,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  Header="Datos del Destino">
            <Grid>
                <Label Name="label4"
                       Height="28"
                       Margin="19,6,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       Content="Organismo" />
                <ComboBox Name="CbxDestino"
                          Width="446"
                          Height="23"
                          Margin="19,29,0,0"
                          ItemsSource="{Binding}"
                          DisplayMemberPath="Organismo"
                          SelectedValuePath="IdOrganismo"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top" />
                <Label Name="label5"
                       Height="28"
                       Margin="19,69,0,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"
                       Content="Expediente:" />
                <TextBox Name="TxtExpDestino"
                         Width="239"
                         Height="23"
                         Margin="86,66,0,0"
                         Text="{Binding ExpDestino}"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <Button Name="BtnAceptar"
                Width="75"
                Height="23"
                Margin="0,354,27,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Content="Aceptar" Click="BtnAceptar_Click" />
        <Button Name="BtnCancelar"
                Width="75"
                Height="23"
                Margin="378,354,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Cancelar" Click="BtnCancelar_Click" />
    </Grid>
</Window>
